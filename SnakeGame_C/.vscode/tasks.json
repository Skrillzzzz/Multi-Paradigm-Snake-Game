{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "build with SDL2",
            "command": "clang",
            "args": [
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "/Users/skrilla/Library/CloudStorage/OneDrive-MMU/Documents/YEAR 3/PROGRAMMING PARADIGMS/SnakeGame_C/src/snake.c",
                "-I/Users/skrilla/Homebrew/local/Cellar/sdl2/2.28.5/include",
                "-L/Users/skrilla/Homebrew/local/Cellar/sdl2/2.28.5/lib",
                "-lSDL2",
                "-lm"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "group": "build",
            "problemMatcher": "$gcc",
            "detail": "Building with SDL2 using Clang"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: clang build active file",
            "command": "/usr/bin/clang",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}